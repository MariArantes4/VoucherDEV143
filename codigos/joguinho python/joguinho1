nota_1=float (input("nota: "))
nota_2=float (input("nota: "))
nota_3=float (input("nota: "))

m1= nota_1+nota_2+nota_3//3
print ("a primeira media é:", m1)

nota_4=float (input("nota: "))
nota_5=float (input("nota: "))
nota_6=float (input("nota: "))

m2= nota_4+nota_5+nota_6//3
print ("a segunda media é:", m2)

m3= m1+m2
m4=m3//2

print ("a media é= ", m4)